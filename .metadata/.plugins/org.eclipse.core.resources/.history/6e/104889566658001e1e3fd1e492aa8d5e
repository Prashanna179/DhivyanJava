package Demo2;

import Demo1.Add;

public class Sample {

	public void h()
	{
		Add a = new Add();
		a.add(4, 5)
	}
	public static void main(String[] args) {
		
		//Non Static Method Access
		Add g = new Add();
		System.out.println(g.add(3, 4));
		
		//Static Method Access
		System.out.println(Add.sub(5, 2));
		
	}

}
