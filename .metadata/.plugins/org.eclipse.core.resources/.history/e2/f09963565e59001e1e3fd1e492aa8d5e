package Inheritance;

public class Child {

	int b = 10;
	
	String ss  ="Dhivyan";
	
	public void morning()
	{
		System.out.println("morning");
	}
	
	public void night()
	{
		System.out.println("night");
	}
	
	
	public static void main(String args[])
	{
		Child c = new Child();
		c.morning();
		c.night();
		System.out.println(c.b);
		System.out.println(c.ss);
		
		Father f = new Father();
	}
}
